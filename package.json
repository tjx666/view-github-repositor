{
    "name": "view-github-repository",
    "displayName": "View GitHub Repository",
    "version": "1.0.2",
    "preview": false,
    "description": "Open npm package GitHub repository straight from VScode",
    "publisher": "YuTengjing",
    "author": {
        "name": "YuTengjing",
        "url": "https://github.com/tjx666",
        "email": "ytj2713151713@gmail.com"
    },
    "icon": "images/logo.png",
    "license": "SEE LICENSE IN LICENSE",
    "badges": [
        {
            "url": "https://img.shields.io/badge/License-MIT-brightgreen.svg",
            "description": "License: MIT",
            "href": "https://github.com/tjx666/view-github-repository/blob/master/LICENSE"
        },
        {
            "url": "https://img.shields.io/badge/PRs-welcome-brightgreen.svg",
            "description": "PRs Welcome",
            "href": "https://github.com/tjx666/view-github-repository"
        }
    ],
    "homepage": "https://github.com/tjx666/view-github-repository/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/tjx666/view-github-repository/issues",
        "email": "ytj2713151713@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/view-github-repository"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "view-gitHub-repository",
        "view-github",
        "view-repository",
        "github",
        "repository",
        "github-repository",
        "npm",
        "package",
        "npm-package",
        "node-package",
        "view-package",
        "view-npm-package",
        "view-node-package"
    ],
    "activationEvents": [
        "onCommand:viewGithubRepository.viewActiveEditorRepository",
        "onCommand:viewGithubRepository.viewPackageJSONRepository"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "viewGithubRepository.viewActiveEditorRepository",
                "title": "view github repository"
            },
            {
                "command": "viewGithubRepository.viewPackageJSONRepository",
                "title": "view github repository (package.json)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run build",
        "compile": "cross-env NODE_ENV=development ts-node --files -P ./tsconfig.webpack.json ./scripts/pack.ts",
        "watch": "yarn run compile -- --watch",
        "test-compile": "rimraf -rf ./out && tsc -p ./tsconfig.test.json",
        "build": "cross-env NODE_ENV=production ts-node --files -P ./tsconfig.webpack.json ./scripts/pack.ts",
        "lint": "eslint -c .eslintrc --ext .ts ./{src,test}/**/*.ts",
        "test": "node ./out/test/runTests.js",
        "test-all": "yarn run lint && yarn run test-compile && yarn run test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint -c .eslintrc"
        ],
        "*.{js,jsx,ts,tsx,json,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "axios": "^0.19.0",
        "builtin-modules": "^3.1.0",
        "fs-extra": "^8.1.0",
        "is-online": "^8.2.1",
        "lodash": "^4.17.15",
        "open": "^7.0.0",
        "package-json": "^6.5.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.0.1",
        "@types/glob": "^7.1.1",
        "@types/lodash": "^4.14.149",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/terser-webpack-plugin": "^2.2.0",
        "@types/vscode": "^1.40.0",
        "@types/webpack": "^4.41.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/yargs": "^13.0.3",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-prettier": "^3.1.1",
        "glob": "^7.1.5",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0",
        "mocha": "^6.2.2",
        "prettier": "^1.19.1",
        "progress-bar-webpack-plugin": "^1.12.1",
        "rimraf": "^3.0.0",
        "terser-webpack-plugin": "^2.2.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3",
        "vscode-test": "^1.2.2",
        "webpack": "^4.41.2",
        "webpack-merge": "^4.2.2",
        "yargs": "^15.0.2"
    }
}
